class Coche:
    def __init__(self,color,marca,modelo,caballosdevapor,puerta,matricula):
        self.color = color
        self.marca = marca
        self.modelo = modelo
        self.caballosdevapor = caballos
        self.puerta = puerta
        self.matricula = matricula
    def get_informacion(self):
        return self.color, self.marca, self.modelo, self.caballosdevapor, self.puerta, self.matricula

class PruebaCoche(Coche):
    def __init__(self,color,marca,modelo,caballosdevapor,puerta,matricula,cambio_color = None):
        super().__init__(color,marca,modelo,caballosdevapor,puerta,matricula)
    def cambio_color(self):
        if self.cambio_color is not None:
            self.cambio_color = cambio_color
        return self.cambio_color

lamborghini = PruebaCoche("negro","Lamborghini","Pegaso","2500CV",4,"4043GBA", "amarillo")
print('El deportivo de color ', lamborghini. color, 'y de marca', lamborghini.marca, 'es el modelo", lamborghini.modelo, '. Además, tiene', lamborghini.caballos,'caballos de vapor,  lamborghini.puerta,'puertas', 'y su matrícula es ', lamborghini.matricula, '. Por último, pero no menos importante,  ha cambiado al color", lamborghini.cambio_color())
